<%- include header %>
		<form role="form" method="post" class="col-md-6" id="reg">
			<div  class="form-group">
				<label for="regname">用户名（长度在2-10位之间）</label>
				<input id="regname" class="form-control" type="text" name="name" required placeholder="username">
			</div>
			<div  class="form-group">
				<label for="regpassword">密码（长度在6-16位之间）</label>
				<input id="regpassword" class="form-control" type="password" name="password" required placeholder="password">
			</div>
  			<div  class="form-group">
				<label for="regpasswordagain">再次输入密码</label>
				<input id="regpasswordagain" class="form-control" type="password" name="password-repeat" required placeholder="password-repeat">
			</div>
			<div  class="form-group">
				<label for="regemail">邮箱</label>
				<input id="regemail" class="form-control" type="email" name="email" required placeholder="Enter email">
			</div>
			<button type="submit" class="btn btn-primary">Register</button>
		</form>
<!-- /.blog-main -->
</div>
<!-- /.row -->
</div>
<!-- /.container -->
</div>

<%- include footer %>